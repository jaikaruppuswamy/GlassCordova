<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ge.capital.cdf.mobile" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <preference name="fullscreen" value="true" />
    <feature name="GlassCore">
        <param name="android-package" value="com.rossgerbasi.cordova.glass.GlassCorePlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GlassGesture">
        <param name="android-package" value="com.rossgerbasi.cordova.glass.GlassGesturePlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GlassTouch">
        <param name="android-package" value="com.rossgerbasi.cordova.glass.GlassTouchPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>DealerGlass</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
